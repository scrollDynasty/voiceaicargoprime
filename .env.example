# Копируйте этот файл в .env и заполните значения

# OpenAI API ключ для генерации AI ответов
OPENAI_API_KEY=your_openai_api_key_here

# Настройки SIP сервера (опционально)
SIP_PORT=5060
RTP_START_PORT=10000

# Настройки AI (опционально)
AI_MODEL=gpt-3.5-turbo
AI_TEMPERATURE=0.7
AI_MAX_TOKENS=150

# Настройки речи (опционально)
WHISPER_MODEL=base
TTS_MODEL=tts_models/en/ljspeech/tacotron2-DDC